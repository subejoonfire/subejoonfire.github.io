{
  "name": "harlan-muradi-portfolio",
  "version": "2.0.0",
  "description": "Enhanced portfolio website for Harlan Muradi - Full Stack Web Developer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "serve": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "vite": "^5.2.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "keywords": [
    "portfolio",
    "react",
    "web-developer",
    "full-stack",
    "responsive-design",
    "modern-web",
    "vite",
    "harlan-muradi"
  ],
  "author": {
    "name": "Harlan Muradi",
    "email": "harlanmuradi@gmail.com",
    "url": "https://github.com/subejoonfire"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/subejoonfire/portfolio.git"
  },
  "homepage": "https://harlanmuradi.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}